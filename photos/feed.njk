---
permalink: photos/feed.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>{{ metadata.photoFeed.title }}</title>
	<subtitle>{{ metadata.photoFeed.subtitle }}</subtitle>
	<link href="{{ metadata.photoFeed.url }}" rel="self"/>
	<link href="{{ metadata.url }}"/>
	<updated>{{ collections.photos | rssLastUpdatedDate }}</updated>
	<id>{{ metadata.photoFeed.id }}</id>
	<author>
		<name>{{ metadata.author.name }}</name>
		<email>{{ metadata.author.email }}</email>
	</author>
	{%- for photo in collections.photos %}
	{% set absolutePostUrl %}{{ photo.url | url | absoluteUrl(metadata.url) }}{% endset %}
	<entry>
		<title>{{ photo.data.title }}</title>
		<link href="{{ absolutePostUrl }}"/>
		<updated>{{ photo.date | rssDate }}</updated>
		<id>{{ absolutePostUrl }}</id>
		<content type="html">
		<![CDATA[
			<figure class="photo-frame">
				<img class="photo-frame__image" src="{{ photo.data.imageSource | imgixPhotoSingle }}" alt="{{ photo.data.imageAlt }}">
				<figcaption class="photo-single__item-caption">{{ photo.data.title }}</figcaption>
			</figure>
		]]>
		</content>
	</entry>
	{%- endfor %}
</feed>
